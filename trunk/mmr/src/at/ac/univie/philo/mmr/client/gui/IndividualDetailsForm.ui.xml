<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style
		type="at.ac.univie.philo.mmr.client.gui.UniverseDetailsForm.DetailStyle"
		src="defaultStyle.css">
	</ui:style>

	<g:VerticalPanel ui:field="mainVPanel" addStyleNames="{style.leftDistance}">
		<g:HTMLPanel>
			<h1 class="{style.heading}">
				Details of Individual:
				<span ui:field='individualName' />
			</h1>
			</g:HTMLPanel>
		<g:HorizontalPanel ui:field="indiNameContainer" spacing="10">
			<g:HTMLPanel ui:field="indiNameHtmlPanel"><div>Ordinary name of Individual:</div></g:HTMLPanel>
			<g:Label ui:field="indiNameLabel"></g:Label>
			<g:Button ui:field="indiNameHelpButton">?</g:Button>
		</g:HorizontalPanel>
		<g:HorizontalPanel ui:field="indiIconContainer" spacing="10">
			<g:HTMLPanel ui:field="indiIconHtmlPanel"><div>Icon:</div></g:HTMLPanel>
			<g:SimplePanel addStyleNames="{style.thickBorder}" ui:field="indiIconHolder"></g:SimplePanel>
			<!-- <g:Button ui:field="changeImage">Change</g:Button> -->
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 